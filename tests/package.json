{
  "name": "gemini-integration-tests",
  "version": "1.0.0",
  "description": "Unit tests for BOT-6453: Gemini Integration Issues",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": ["gemini", "llm", "testing", "botium"],
  "author": "Brandon Young",
  "license": "ISC",
  "devDependencies": {
    "jest": "^29.7.0",
    "@types/jest": "^29.5.5"
  },
  "dependencies": {
    "axios": "^1.9.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "../botium-box/packages/botium-box-shared/llm/**/*.js"
    ],
    "testMatch": [
      "**/*.test.js"
    ],
    "setupFilesAfterEnv": ["<rootDir>/setup.js"]
  }
} 